{
  "pages": [
    //pages数组中第一项表示应用启动页,
    {
      "path": "pages/main/main",
      "style": {
        "navigationBarBackgroundColor": "#3A74F1",
        "app-plus": {
          "titleNView": {
            "searchInput": {
              "align": "center",
              "backgroundColor": "#89AEFF",
              "borderRadius": "17px", // 只能用px作单位
              "placeholder": "输入商品名，通用名，批准文号",
              "width": "200px"
            },
            "buttons": [
              {
                "color": "#fff",
                "colorPressed": "#eee",
                "float": "left",
                "fontSize": "22px",
                "width": "22px",
                "fontSrc": "/static/fontIcon/iconfont.ttf", // 字体文件
                "text": "\ue601" // 字体图标\u 开头，加上字体图标unicode后面四位
              },
              {
                "color": "#fff",
                "colorPressed": "#eee",
                "float": "right",
                "fontSize": "22px",
                "width": "22px",
                "fontSrc": "/static/fontIcon/iconfont.ttf", // 字体文件
                "text": "\ue602" // 字体图标\u 开头，加上字体图标unicode后面四位
              }
            ]
          }
        }
      }
    },
    {
      "path": "pages/login/login",
      "style": {
        "navigationBarTitleText": "",
        "navigationBarBackgroundColor": "#fff",
        "app-plus": {
          "titleNView": {
            "type": "back",
            "titleColor": "#000000"
          }
        }
      }
    },
    {
      "path": "pages/smsValidte/smsValidte",
      "style": {
        "navigationBarTitleText": "",
        "navigationBarBackgroundColor": "#fff",
        "app-plus": {
          "titleNView": {
            "type": "back",
            "titleColor": "#000000"
          }
        }
      }
    },
    {
      "path": "pages/user/user",
      "style": {
        "navigationBarTitleText": "我的"
      }
    }
  ],
  "tabBar": {
    "color": "#7a7e83",
    "selectedColor": "#0faeff",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/main/main",
        "text": "首页",
        "iconPath": "static/img/home.png",
        "selectedIconPath": "static/img/homeHL.png"
      },
      {
        "pagePath": "pages/user/user",
        "text": "我的",
        "iconPath": "static/img/user.png",
        "selectedIconPath": "static/img/userHL.png"
      }
    ]
